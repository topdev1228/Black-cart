#!/usr/bin/env bash
ESC='\033['
YELLOW="${ESC}1;33m"
RED="${ESC}0;31m"
GREEN="${ESC}0;32m"
BLUE="${ESC}0;34m"
NC="${ESC}0m"
BOLD="${ESC}1m"

# Whitelist functions
/usr/local/bin/php -d auto_prepend_file='' "`dirname $0`/whitelist_functions.php"

# Test nginx config
/usr/sbin/nginx -t -c /etc/nginx/nginx.conf